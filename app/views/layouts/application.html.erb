<!DOCTYPE html>
<html>
<head>
  <title>AskJenn</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="jennphoto"><img src="/images/askjenn.jpg" alt="ask jenn" /></div>

  <% flash.each do |name, msg| %>
  <% if msg.is_a?(String) %>
  <%= content_tag :div, msg, :class => "flash_#{name}" %>
  <% end %>
<% end %>

<%= yield %>

</body>
</html>
